FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 0.3278688561020396;//13921.003211818179;

boundaryField
{
    airfoil
    {
        type            omegaWallFunction;
	value		$internalField;
    }
    inlet
    {
        type            freestream;
        freestreamValue $internalField;
    }
    outlet
    {
        type            freestream;
        freestreamValue $internalField;       
    }
    defaultFaces
    {
        type            empty;
    }
}


